{
  "schemaVersion": 1,
  "id": "unichantments",
  "version": "${version}",

  "name": "Unique Enchantments",
  "description": "Implied by the name, this mod adds a few unique enchantments that you may not find in the Enchanting Table!",
  "authors": [
    "Slayerdev!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.slayerdev.unichantments.Unichantments"
    ],
    "client": [
      "net.slayerdev.unichantments.UnichantmentsClient"
    ]
  },
  "mixins": [
    "unichantments.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
